%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Adds Results to Bars labeled by #1, #2
%
% Label #1 is used for saveResultsFirst
% Label #2 is used for saveResultsSecond
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newcommand{\addBarPlotResults}[3]{
    % ACCURACY
    \addplot[style={bblue, fill=bblue, mark=none}]
    coordinates 
    {
        (#1,  \AccuracyFirst)
        (#2,  \AccuracySecond)
        (#3,  \AccuracyThird)
    };
    
    % SENSITIVITY
    \addplot[style={rred, fill=rred, mark=none}]
    coordinates 
    {
        (#1,  \SensitivityFirst)
        (#2,  \SensitivitySecond)
        (#3,  \SensitivityThird)
    };
    
    % PRECISION
    \addplot[style={ggreen, fill=ggreen, mark=none}]
    coordinates 
    {
        (#1,  \PrecisionFirst)
        (#2,  \PrecisionSecond)
        (#3,  \PrecisionThird)
    };
    
    % FMEASURE
    \addplot[style={ppurple, fill=ppurple, mark=none}]
    coordinates 
    {
        (#1,  \FmeasureFirst)
        (#2,  \FmeasureSecond)
        (#3,  \FmeasureThird)
    };
}

