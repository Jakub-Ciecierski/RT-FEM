%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Adds Results to Bars labeled by #1, #2
%
% Label #1 is used for saveResultsFirst
% Label #2 is used for saveResultsSecond
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newcommand{\addBarPlotResults}[3]{
    % ACCURACY
    \addplot[style={bblue, fill=bblue, mark=none}]
    coordinates 
    {
        (#1,  \AccuracyFirst)
        (#2,  \AccuracySecond)
        (#3,  \AccuracyThird)
    };
    
    % SENSITIVITY
    \addplot[style={rred, fill=rred, mark=none}]
    coordinates 
    {
        (#1,  \SensitivityFirst)
        (#2,  \SensitivitySecond)
        (#3,  \SensitivityThird)
    };
    
    % PRECISION
    \addplot[style={ggreen, fill=ggreen, mark=none}]
    coordinates 
    {
        (#1,  \PrecisionFirst)
        (#2,  \PrecisionSecond)
        (#3,  \PrecisionThird)
    };
    
    % FMEASURE
    \addplot[style={ppurple, fill=ppurple, mark=none}]
    coordinates 
    {
        (#1,  \FmeasureFirst)
        (#2,  \FmeasureSecond)
        (#3,  \FmeasureThird)
    };
}

\newcommand{\addBarPlotFourResults}[4]{
    % ACCURACY
    \addplot[style={bblue, fill=bblue, mark=none}]
    coordinates 
    {
        (#1,  \AccuracyFirst)
        (#2,  \AccuracySecond)
        (#3,  \AccuracyThird)
        (#4,  \AccuracyFourth)
    };
    
    % SENSITIVITY
    \addplot[style={rred, fill=rred, mark=none}]
    coordinates 
    {
        (#1,  \SensitivityFirst)
        (#2,  \SensitivitySecond)
        (#3,  \SensitivityThird)
        (#4,  \SensitivityFourth)
    };
    
    % PRECISION
    \addplot[style={ggreen, fill=ggreen, mark=none}]
    coordinates 
    {
        (#1,  \PrecisionFirst)
        (#2,  \PrecisionSecond)
        (#3,  \PrecisionThird)
        (#4,  \PrecisionFourth)
    };
    
    % FMEASURE
    \addplot[style={ppurple, fill=ppurple, mark=none}]
    coordinates 
    {
        (#1,  \FmeasureFirst)
        (#2,  \FmeasureSecond)
        (#3,  \FmeasureThird)
        (#4,  \FmeasureFourth)
    };
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Adds needed style to pgfplot
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newcommand{\setPlotStyle}[0]{
    \pgfplotsset{xticklabel style={text width=2em, align=center}}
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Starts groupplot with given style
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newcommand{\beginGroupPlot}[0]{
    \begin{groupplot}[
        group style={group size= 2 by 2,  vertical sep=50pt},
        height = 5.5cm, 
        ybar=4*\pgflinewidth, 
        ymin=0, 
        ymax=1,
        width  = 0.50*\textwidth, 
        enlarge x limits={abs=0.85cm}, 
        major x tick style = transparent, 
        ymajorgrids = true, 
        symbolic x coords={
            r=10,
            r=30,
            r=40,
            A=10,
            A=30,
            A=40,
            N=20 F=10,
            N=20 F=20,
            N=20 F=40,
            N=20 F=80
        },
        xtick = data, 
        scaled y ticks = false, 
        legend cell align=center, 
        legend columns=1,
        legend style={
            at={(1.25, 0.45)}, 
            anchor=north,
        }
        ]
    }%beginGroupPlot
    
    \newcommand{\beginGroupPlotFour}[0]{
        \begin{groupplot}[
            group style={group size= 2 by 2,  vertical sep=50pt},
            height = 5.5cm, 
            ybar=4*\pgflinewidth, 
            ymin=0, 
            ymax=1,
            width  = 0.65*\textwidth, 
            enlarge x limits={abs=0.85cm}, 
            major x tick style = transparent, 
            ymajorgrids = true, 
            symbolic x coords={
                r=10,
                r=30,
                r=40,
                A=10,
                A=30,
                A=40,
                N=20 F=10,
                N=20 F=20,
                N=20 F=40,
                N=20 F=80
            },
            xtick = data, 
            scaled y ticks = false, 
            legend cell align=center, 
            legend columns=1,
            legend style={
                at={(1.25, 0.45)}, 
                anchor=north,
            }
            ]
        }%beginGroupPlot
    
